modules = ["python-3.11", "rust-stable"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["c2patool", "freetype", "lcms2", "libimagequant", "libjpeg", "libtiff", "libwebp", "libxcrypt", "openjpeg", "tcl", "tk", "zlib"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "originstamp-cli"

[[workflows.workflow]]
name = "originstamp-cli"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python test_originstamp.py"

[workflows.workflow.metadata]
outputType = "console"
